{
  "name": "agnos-rest-api",
  "version": "0.0.1",
  "description": "REST API for Agnos",
  "main": "index.js",
  "engines": {
    "node": "18.12.x"
  },
  "scripts": {
    "build": "ttsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "docker:build:dev": "docker build --target development -t agnos-rest-api:dev .",
    "docker:build:prod": "docker build --target production -t agnos-rest-api .",
    "docker:compose:test": "docker-compose -f docker-compose.test.yml up --build --abort-on-container-exit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "packages:outdated": "npm outdated",
    "packages:audit": "npm audit",
    "prettier-format": "prettier --write src/**/*.ts",
    "prepare": "husky install",
    "start": "NODE_ENV=production node build/src/index.js",
    "test": "jest --testTimeout=10000 --coverage"
  },
  "author": "Franklin Chieze",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.23",
    "@types/pino": "^7.0.5",
    "@types/request": "^2.48.8",
    "@types/response-time": "^2.3.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "ttypescript": "1.5.13",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "bcrypt": "^5.1.0",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.0",
    "express": "^4.17.3",
    "express-jwt": "^6.1.1",
    "express-oauth2-jwt-bearer": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongoose": "^6.2.9",
    "nanoid": "^3.3.3",
    "openapi-types": "^10.0.0",
    "pino": "^7.9.2",
