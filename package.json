    "request": "^2.88.2",
    "response-time": "^2.3.2",
    "slugify": "^1.6.5",
    "socket.io": "^4.5.0",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-express": "^4.3.0",
    "vm2": "^3.9.9",
    "zod": "^3.14.3"
