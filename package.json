    "@services/component": "src/features/component/service/component.service",
    "@services/design": "src/features/design/service/design.service",
    "@services/installation": "src/features/installation/service/installation.service",
    "@services/instance": "src/features/instance/service/instance.service",
    "@services/membership": "src/features/membership/service/membership.service",
    "@services/model": "src/features/model/service/model.service",
