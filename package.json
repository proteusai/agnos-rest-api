  "prettier": {
    "parser": "typescript",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.ts": [
      "eslint",
      "prettier --write"
    ]
  }
}
